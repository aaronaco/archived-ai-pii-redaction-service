{
  "name": "ai-pii-redaction-service",
  "version": "1.0.0",
  "description": "High-performance bidirectional proxy that sanitizes LLM prompts and responses using ONNX-powered PII detection",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "node scripts/download-model.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/"
  },
  "keywords": [
    "pii",
    "redaction",
    "privacy",
    "llm",
    "proxy",
    "onnx"
  ],
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/reply-from": "^12.5.0",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "ioredis": "^5.9.2",
    "onnxruntime-node": "^1.23.2",
    "pino-pretty": "^13.1.3",
    "sharp": "^0.34.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "eslint": "^9.39.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["sharp", "onnxruntime-node", "esbuild", "protobufjs"]
  }
}
